{
	"manifest_version": 2,
	"name": "Google Translate for Safari",
	"description": "description",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"background": {
		"scripts": [
			"shared.js",
			"background.js"
		]
	},
	"content_scripts": [],
	"permissions": [
		"activeTab",
		"tabs",
		"storage",
		"<all_urls>"
	],
	"browser_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "Google Translate for Safari"
	}
}